{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/wo0lien/cosmoBot/internal/config/config-struct",
  "$ref": "#/$defs/ConfigStruct",
  "$defs": {
    "ConfigStruct": {
      "properties": {
        "responseMethodByEventType": {
          "additionalProperties": {
            "$ref": "#/$defs/ResponseMethod"
          },
          "type": "object"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "responseMethodByEventType"
      ]
    },
    "ResponseMethod": {
      "properties": {
        "discordId": {
          "type": "string"
        },
        "channelType": {
          "type": "integer"
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "discordId",
        "channelType"
      ]
    }
  }
}